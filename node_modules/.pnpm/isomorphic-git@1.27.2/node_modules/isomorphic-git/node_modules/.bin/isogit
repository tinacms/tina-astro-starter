#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/isomorphic-git@1.27.2/node_modules/isomorphic-git/node_modules:/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/isomorphic-git@1.27.2/node_modules:/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/isomorphic-git@1.27.2/node_modules/isomorphic-git/node_modules:/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/isomorphic-git@1.27.2/node_modules:/Users/jackpettit/Developer/tina-starters/tina-astro-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.cjs" "$@"
else
  exec node  "$basedir/../../cli.cjs" "$@"
fi
